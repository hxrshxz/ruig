{
  "name": "ruig",
  "version": "0.1.0",
  "description": "React UI Generator - A tool to generate React user interfaces effortlessly.",
  "author": "Emmanuel Paul Elom <scansioquielom@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.70",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.0.1",
    "cpy": "^11.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.5",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "terser": "^5.27.1",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "start": "npm run build:workers && react-scripts start",
    "build": "npm run build:workers && GENERATE_SOURCEMAP=false react-scripts build && find build -type f -name '*.js' -exec terser {} -o {} --compress --mangle toplevel \\;",
    "format": "prettier --write .",
    "lint": "eslint .",
    "build:workers": "npx webpack --config webpack.cacher.config.js && npx webpack --config webpack.extensionprovider.config.js",
    "lint:autofix": "eslint . --fix",
    "prettier": "prettier .",
    "prettier:fix": "prettier --write .",
    "build:dts": "npm run build:workers && tsc -p tsconfigtype.json && find ruig-type -type f -name '*.js' -exec terser {} -o {} --compress --mangle toplevel \\; && cpy \"src/assets/**/*\" \"ruig-type/assets\"",
    "build:dts:debug": "npm run build:workers && tsc -p tsconfigtypedebug.json && cpy \"src/assets/**/*\" \"ruig-type/assets\"",
    "setupdev": "npm install -f",
    "preview": "npm run build && serve -s build",
    "buildAndPreview": "npm run build && npm run preview",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.6.5",
    "html-to-image": "^1.11.11",
    "idb": "^8.0.0",
    "jszip": "^3.10.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "ruig-type": "^0.1.17"
  }
}
